<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
		PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
		"http://tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite>


	<!-- Attention! When changing these values, also change GeodeskIdServiceImpl and CommonLayout! -->
	<rule>
		<from>(.*)/desk/([^/]*)/(.*)$</from>
		<to>$1/$3</to>
		<set name="desk">$2</set>
	</rule>
</urlrewrite>
